<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>disable</Nullable>
        <LangVersion>10</LangVersion>
    </PropertyGroup>
    
    <PropertyGroup>
        <OutputType>Library</OutputType>
        <TargetFramework>netstandard2.1</TargetFramework>
        <PackageId>Proxyl</PackageId>
        <Version>1.0.0$(VersionSuffix)</Version>
        <Authors>cmelende</Authors>
        <PackageDescription>Proxyl, the proxy list package</PackageDescription>
        <RepositoryUrl>https://github.com/cmelende/Proxyl</RepositoryUrl>
    </PropertyGroup>
</Project>
